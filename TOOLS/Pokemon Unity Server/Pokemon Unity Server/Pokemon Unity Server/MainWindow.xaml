<Window x:Class="Pokemon_Unity_Server.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Pokemon_Unity_Server"
        mc:Ignorable="d"
        Title="Pokemon Unity Server" Height="459.333" Width="800" Background="#FF1C1C1C">
    <Grid>
        <RichTextBox x:Name="ConsoleText" HorizontalAlignment="Left" Height="364" VerticalAlignment="Top" Width="357" Foreground="#FFBABABA" Background="#FF1C1C1C" Margin="10,10,0,0" IsReadOnly="True" Block.LineHeight="1">
            <FlowDocument>
                <Paragraph>
                    <Run Text="RichTextBox"/>
                </Paragraph>
            </FlowDocument>
        </RichTextBox>
        <Button x:Name="StartServer" Content="Start Server" HorizontalAlignment="Left" Margin="10,379,0,0" VerticalAlignment="Top" Width="357"/>
    </Grid>
</Window>
